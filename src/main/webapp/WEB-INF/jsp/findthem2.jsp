<script type="text/javascript">
	function previousPage() {
		var lastPage = "${ page - 1 }";
		if (lastPage <= 0) {
			$("[name='page']").val("1");
		} else {

			$("[name='page']").val("${ page - 1 }");
		}
		$("form").submit();
	}
	function nextPage() {
		$("[name='page']").val("${ page + 1 }");
		$("form").submit();
	}
	function locatePage() {
		$("[name='page']").val($("[name='page_located']").val());
		$("form").submit();
	}
	function keywordInit() {
		$("[name='keyword']").val("");
	}
</script>
